@model IEnumerable<WebApplication1.Models.MateriaModels>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Alumnos", "../Alumno/Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre_Materia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Materia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nota_Materia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Legajo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_Materia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Materia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nota_Materia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Legajo)
        </td>
        <td>
            @Html.ActionLink("Details", "Detail", new {  nombre=item.Nombre_Materia  }) |
        </td>
    </tr>
}

</table>
